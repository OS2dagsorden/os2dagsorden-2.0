name = "OS2Dagsorden Ultimate Cron configuration"
description = "configuration of cron functions with modified schedule - in order to save resources on heavy jobs"
core = "7.x"
package = "OS2Dagsorden"
php = "5.2.4"
version = "7.x-1.0-alpha"
project = "os2dagsorden_ultimate_cron_configuration"
dependencies[] = "ctools"
dependencies[] = "strongarm"
dependencies[] = "ultimate_cron"
features[ctools][] = "strongarm:strongarm:1"
features[ctools][] = "ultimate_cron:default_ultimate_cron_functions:1"
features[ultimate_cron][] = "os2dagsorden_access_helper_cron"
features[variable][] = "ultimate_cron_catch_up"
features[variable][] = "ultimate_cron_cleanup_log"
features[variable][] = "ultimate_cron_poorman"
features[variable][] = "ultimate_cron_queue_polling_latency"
features[variable][] = "ultimate_cron_rule"
features[variable][] = "ultimate_cron_simultaneous_connections"
